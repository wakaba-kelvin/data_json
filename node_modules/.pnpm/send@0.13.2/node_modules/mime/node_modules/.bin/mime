#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/kiman/Final-project/data_json/node_modules/.pnpm/mime@1.3.4/node_modules/mime/node_modules:/mnt/c/Users/kiman/Final-project/data_json/node_modules/.pnpm/mime@1.3.4/node_modules:/mnt/c/Users/kiman/Final-project/data_json/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/kiman/Final-project/data_json/node_modules/.pnpm/mime@1.3.4/node_modules/mime/node_modules:/mnt/c/Users/kiman/Final-project/data_json/node_modules/.pnpm/mime@1.3.4/node_modules:/mnt/c/Users/kiman/Final-project/data_json/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
